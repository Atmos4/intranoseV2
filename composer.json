{
  "name": "atmos4/intranose-v2",
  "type": "project",
  "license": "MIT",
  "require": {
    "doctrine/orm": "^2.11.0",
    "symfony/cache": "^5.4",
    "doctrine/migrations": "^3.6",
    "phpmailer/phpmailer": "^6.8",
    "ramsey/uuid": "^4.7",
    "vlucas/phpdotenv": "^5.5",
    "monolog/monolog": "^3.4",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "classmap": [
      "database/models/",
      "engine/core/",
      "engine/validation/",
      "app/services",
      "engine/api/"
    ],
    "files": [
      "engine/helpers.php"
    ]
  },
  "scripts": {
    "install:all": [
      "@composer install",
      "@db:migrate"
    ],
    "db:migrate": "@php bin/doctrine --no-interaction migrations:migrate",
    "db:diff": "@php bin/doctrine --no-interaction migrations:diff"
  }
}
