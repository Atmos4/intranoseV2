{
  "name": "intranose",
  "version": "1.0.0",
  "repository": "https://github.com/Atmos4/intranoseV2.git",
  "author": "Atmos4 <artrist.perrin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "setup": "composer install && yarn db:migrate",
    "update:all": "yarn update:autoload && yarn db:migrate",
    "db:migrate": "php bin/doctrine --no-interaction migrations:migrate",
    "update:autoload": "composer dump-autoload",
    "db:diff": "php bin/doctrine migrations:diff --allow-empty-diff",
    "db:execute": "php bin/doctrine migrations:execute",
    "db:list": "php bin/doctrine migrations:list"
  }
}
