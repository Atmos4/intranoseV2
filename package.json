  "license": "MIT",
  "scripts": {
    "setup": "composer install && yarn update:db",
    "update": "yarn update:autoload && yarn update:db",
    "update:db": "php bin/doctrine --no-interaction migrations:migrate",
    "update:autoload": "composer dump-autoload",
    "diff": "php bin/doctrine migrations:diff --allow-empty-diff"
  }
}
